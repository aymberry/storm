	"-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mypage">
	<select id="mypageList"	parameterType="int"		resultType="mVo">
		select  usr_no as usr_no  
		from    user_table  
		where 	USR_ENAME=?
	</select>

</mapper>